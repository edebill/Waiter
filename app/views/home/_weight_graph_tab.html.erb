<script id="source">
    function show_weight_graph() {
        console.log("rendering weight graph");
        $.getJSON("/biometrics/weight.json", function(data) {
            $.plot($("#placeholder"), [data], {
                xaxis: {
                    mode: "time"
                }   
            });
        });
    }
</script>
